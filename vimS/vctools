let SessionLoad = 1
if &cp | set nocp | endif
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/Dropbox/proj/VCtools
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +41 lib/App/VC/Command/info.pm
badd +232 lib/App/VC/Command.pm
badd +3 Changes
badd +60 lib/App/VC.pm
badd +78 lib/App/VC/CustomCommand.pm
