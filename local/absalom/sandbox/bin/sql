#! /bin/bash

db=${1:-dev}
case $db in
	dev)	db=localdev
			;;
	stg)	db=cestg-tunnel
			;;
	prod)	db=ceprod4-tunnel
			;;
	rpt)	db=ceprod4-tunnel
			;;
	log)	db=redshift-ceprod-tunnel
			;;
	lstg)	db=redshift-cestg-tunnel
			;;
	utest)	db=cetest
			;;
esac
echo "$0: connecting to DB: $db"

cd $CEROOT
make tunnels
bin/db $db
