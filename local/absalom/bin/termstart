#!/bin/bash

. ~/bin/bash_funcs

#ssh-add
#ssh-add -l

if ! dropbox start
then
	if yn "Reinstall dropbox?"
	then
		dropbox install force
	fi
fi

cd ~/workproj/CE
vagrant up

if screen -ls | grep base
then
	cmd="screen -x base"
else
	cmd="screen -S base"
fi
myterm -s Absalom city-spires 125x50+7+30 $cmd

