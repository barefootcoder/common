#! /bin/bash

echo -e "\n\n\n\n"

timerfile=timer-new
ls -lh `find ~/timer/ -name $timerfile -o -name ".$timerfile.sw?" ; find ~/.purple/ -name '[abs]*.xml' | grep -v ' '`

echo "===="
ps | head -n1
vims_pids=$(/usr/bin/pgrep -f bin/vimS)
[[ $vims_pids ]] && ps h -p $vims_pids
[[ -d ~/workproj/cheops ]] && vagrant-exe switchover-status-part2
